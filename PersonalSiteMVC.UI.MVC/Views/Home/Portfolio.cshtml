
@{
    ViewBag.Title = "Portfolio";
}

@section stylesheets {
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.13.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="http://resources/demos/style.css">
}

<section class="background-gray-lightest">
